<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="igc2.cs262.calvin.edu.lab06">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="igc2.cs262.calvin.edu.lab06.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="igc2.cs262.calvin.edu.lab06.OrderActivity"
            android:label="@string/title_activity_order"
            android:parentActivityName="igc2.cs262.calvin.edu.lab06.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="igc2.cs262.calvin.edu.lab06.MainActivity" />
        </activity>
        <activity
            android:name="igc2.cs262.calvin.edu.lab06.SettingsActivity"
            android:label="@string/title_activity_settings"
            android:parentActivityName="igc2.cs262.calvin.edu.lab06.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="igc2.cs262.calvin.edu.lab06.MainActivity" />
        </activity>
    </application>

</manifest>